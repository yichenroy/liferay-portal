				</execute>

				<execute macro="User#tearDownCP" />

				<execute macro="Role#tearDownCP" />

				<execute macro="Site#tearDownCP" />

				<execute macro="Suborganization#tearDownCP" />

				<execute macro="Organization#tearDownCP" />
