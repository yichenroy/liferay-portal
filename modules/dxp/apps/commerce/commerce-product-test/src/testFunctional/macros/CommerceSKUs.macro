<definition>
	<command name="addEntry">
		<execute function="Click" locator1="Button#PLUS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="${key_menuItem}">
			<var name="key_menuItem" value="${menuItem}" />
		</execute>

		<execute macro="CommerceEntry#addSKUsEntry">
			<var name="optionText" value="${optionText}" />
			<var name="optionValue" value="${optionValue}" />
			<var name="productSKU" value="${productSKU}" />
			<var name="selectValue" value="${selectValue}" />
		</execute>
	</command>
</definition>