<definition component-name="portal-commerce">
	<property name="portal.upstream" value="false" />
	<property name="test.run.environment" value="EE" />
	<property name="testray.main.component.name" value="Commerce" />

	<set-up>
		<execute macro="TestCase#setUpPortalInstance" />

		<execute macro="User#firstLoginPG" />
	</set-up>

	<tear-down>
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="PortalInstances#tearDownCP" />
			</then>
			<else>
				<execute macro="Navigator#openURL" />

				<execute macro="ProductMenu#gotoSitesCommerce">
					<var name="portlet" value="Catalog" />
				</execute>

				<if>
					<condition function="IsElementNotPresent" locator1="CommerceNavigation#EMPTY_SEARCH_CONTAINER" />
					<then>
						<execute macro="CommerceCatalog#moveCatalogEntriesToRecycleBin" />
					</then>
				</if>

				<execute macro="User#logoutPG" />
			</else>
		</if>
	</tear-down>

	<command ignore="true" name="AssignCheckboxAndRadioOptionToGroupedCatalogEntry" priority="5">
		<execute test-case="CPCommerceOptions#AddSKUContributorCheckboxOptionsEntry" />

		<execute macro="CommerceNavigator#gotoEntry">
			<var name="entryTitle" value="Checkbox Option 1 Title" />
			<var name="portlet" value="Options" />
		</execute>

		<execute macro="CommerceEntry#gotoEntryMenu">
			<var name="menuTab" value="Values" />
		</execute>

		<for list="1,2,3" param="entryNumber">
			<execute macro="CommerceOptions#addOptionValueEntry">
				<var name="optionValuePriority" value="1" />
				<var name="optionValueTitle" value="Value ${entryNumber} Title" />
			</execute>
		</for>

		<execute test-case="CPCommerceOptions#AddSKUContributorRadioOptionsEntry" />

		<execute macro="CommerceNavigator#gotoEntry">
			<var name="entryTitle" value="Radio Option 1 Title" />
