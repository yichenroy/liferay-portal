				<var name="optionValuePriority" value="1" />
				<var name="optionValueTitle" value="Value ${entryNumber} Title" />
			</execute>
		</for>

		<execute test-case="CPCommerceCatalog#AddGroupedCatalogEntry" />

		<execute macro="ProductMenu#gotoSitesCommerce">
			<var name="portlet" value="Catalog" />
		</execute>

		<execute macro="CommerceNavigator#gotoEntry">
			<var name="entryTitle" value="Grouped Product 1 Title" />
		</execute>

		<execute macro="CommerceEntry#gotoEntryTab">
			<var name="menuTab" value="Options" />
		</execute>

		<for list="Checkbox,Radio" param="optionType">
			<execute function="Click" locator1="Button#PLUS" />\

			<execute macro="CommerceItemSelector#selectEntry">
				<var name="entryTitle" value="${optionType} Option 1 Title" />
			</execute>
