<definition component-name="portal-commerce">
	<property name="portal.upstream" value="false" />
	<property name="test.run.environment" value="EE" />
	<property name="testray.main.component.name" value="Commerce" />

	<set-up>
		<execute macro="TestCase#setUpPortalInstance" />

		<execute macro="User#firstLoginPG" />
	</set-up>

	<tear-down>
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="PortalInstances#tearDownCP" />
			</then>
			<else>
				<execute macro="CommerceNavigator#gotoSitesCommerce">
					<var name="portlet" value="Catalog" />
				</execute>

				<if>
					<condition function="IsElementNotPresent" locator1="CommerceNavigation#EMPTY_SEARCH_CONTAINER" />
					<then>
						<execute macro="CommerceCatalog#moveCatalogEntriesToRecycleBin" />
					</then>
				</if>

				<execute macro="User#logoutPG" />
			</else>
		</if>
	</tear-down>

	<command ignore="true" name="AddSkuToGroupedCatalogEntryWithChecboxAndRadioOptionAssociated" priority="5">
		<execute test-case="CommerceOptionsRel#AssignCheckboxAndRadioOptionToGroupedCatalogEntry" />

		<execute macro="CommerceEntry#gotoEntryMenu">
			<var name="menuTab" value="SKUs" />
		</execute>

		<execute macro="CommerceSKUs#addEntry">
			<var name="optionValue" value="Value 2 Title" />
			<var name="plusMenu" value="Add SKU" />
			<var name="productSKU" value="PRDCTSKU" />
		</execute>

		<execute macro="CommerceEntry#publishEntry" />

		<execute function="Click" locator1="Icon#BACK" />

		<execute function="AssertElementPresent" locator1="CommerceNavigation#ENTRY_TITLE" value1="${entryTitle}">
			<var name="key_entryTitle" value="PRDCTSKU" />
		</execute>

		<for list="checkbox,radio" param="type">
			<execute function="AssertElementPresent" locator1="CommerceNavigation#CPOPTIONS_STATUS" >
				<var name="key_cpOption" value="${type}" />
				<var name="key_cpOptionStatus" value="Value 2 Title" />
			</execute>
		</for>
	</command>

