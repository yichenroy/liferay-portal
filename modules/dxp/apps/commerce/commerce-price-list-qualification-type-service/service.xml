<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder auto-namespace-tables="false" package-path="com.liferay.commerce.price.list.qualification.type">
	<author>Alessio Antonio Rendina</author>
	<namespace>Commerce</namespace>
	<entity local-service="true" name="CommercePriceListUserRel" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="commercePriceListUserRelId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column db-name="CPLQualificationTypeRelId" name="commercePriceListQualificationTypeRelId" type="long" />
		<column name="classNameId" type="long" />
		<column name="classPK" type="long" />
		<column name="lastPublishDate" type="Date" />

		<!-- Order -->

		<order by="desc">
			<order-column name="createDate" />
		</order>

		<!-- Finder methods -->

		<finder name="CommercePriceListQualificationTypeRelId" return-type="Collection">
			<finder-column name="commercePriceListQualificationTypeRelId" />
		</finder>
		<finder name="C_C" return-type="Collection">
			<finder-column name="commercePriceListQualificationTypeRelId" />
			<finder-column name="classNameId" />
		</finder>
		<finder name="C_C_C" return-type="Collection">
			<finder-column name="commercePriceListQualificationTypeRelId" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>

		<!-- References -->

		<reference entity="ExpandoRow" package-path="com.liferay.portlet.expando" />
	</entity>
</service-builder>