<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.portal.resiliency.spi.model.SPIDefinition" />
	<class name="com.liferay.portal.resiliency.spi.model.impl.SPIDefinitionImpl" table="SPIDefinition">
		<id name="spiDefinitionId" type="long">
			<generator class="assigned" />
		</id>
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="connectorAddress" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="connectorPort" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="jvmArguments" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="portletIds" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="servletContextNames" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="typeSettings" type="com.liferay.portal.dao.orm.hibernate.StringClobType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="statusMessage" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>