schema {
	query: QueryType
}

type QueryType {
	processes(
		companyId: Long!,
		keyword: String,
		size: Int!,
		start: Int!
	): WorkflowProcessBag
}

type WorkflowInstance {
	name: String!
}

type WorkflowProcess {
	instancesCount: Int!
	instancesOntime: Int!
	instancesOverdue: Int!
	processId: Long!
	title: String!
}

type WorkflowProcessBag {
	total: Long
	workflowProcesses: [WorkflowProcess]
}

type WorkflowTask {
	name: String!
}