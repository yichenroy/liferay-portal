{
	"dependencies": {
		"autobind-decorator": "2.4.0",
		"axios": "0.18.0",
		"classnames": "2.2.6",
		"clay-charts-react": "2.4.1",
		"custom-event-polyfill": "1.0.6",
		"moment": "2.24.0",
		"numeral": "2.0.6",
		"path-to-regexp": "3.0.0",
		"qs": "6.7.0",
		"react": "16.8.6",
		"react-dom": "16.8.6",
		"react-router-dom": "5.0.1",
		"react-text-mask": "5.4.3",
		"string.prototype.startswith": "0.2.0",
		"text-mask-addons": "3.8.0"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/main/resources/META-INF/resources/js/**/components/**/*.js"
		],
		"coverageDirectory": "build/coverage",
		"coverageThreshold": {
			"global": {
				"branches": 70,
				"functions": 70,
				"lines": 70
			}
		},
		"moduleNameMapper": {
			"frontend-js-web/liferay/toast/commands/OpenToast.es": "<rootDir>/test/mock/openToast.js",
			"react-text-mask": "<rootDir>/test/mock/reactTextMask.js"
		},
		"setupFilesAfterEnv": [
			"<rootDir>/test/setup.js"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"testMatch": [
			"<rootDir>/test/components/*.js",
			"<rootDir>/test/components/**/*.js",
			"<rootDir>/test/shared/**/*.js"
		],
		"testURL": "http://liferay.com",
		"timers": "fake"
	},
	"main": "js/index.js",
	"name": "portal-workflow-metrics-web",
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts check",
		"doc:code": "./node_modules/.bin/jsdoc -c ./jsdoc.json",
		"format": "liferay-npm-scripts fix",
		"test_DISABLED": "liferay-npm-scripts test"
	},
	"version": "2.0.0"
}
