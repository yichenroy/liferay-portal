{
	"dependencies": {
		"autobind-decorator": "2.4.0",
		"clay-charts-react": "2.4.1",
		"custom-event-polyfill": "1.0.6",
		"react": "16.6.3",
		"react-dom": "16.6.3"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-jest": "23.6.0",
		"babel-plugin-module-resolver": "3.1.1",
		"babel-plugin-transform-decorators-legacy": "1.3.5",
		"babel-preset-env": "1.7.0",
		"babel-preset-react": "6.24.1",
		"check-source-formatting": "2.0.14",
		"copyfiles": "2.1.0",
		"cross-env": "5.2.0",
		"enzyme": "3.8.0",
		"enzyme-adapter-react-16": "1.7.1",
		"enzyme-to-json": "3.3.5",
		"eslint": "5.14.1",
		"eslint-config-liferay": "2.0.18",
		"eslint-plugin-react": "7.12.4",
		"eslint-plugin-wyze": "3.4.0",
		"jest": "23.6.0",
		"jest-canvas-mock": "1.1.0",
		"jest-extended": "0.11.1",
		"jsdoc": "3.5.5",
		"jsdoc-export-default-interop": "0.3.1",
		"liferay-npm-bundler": "2.5.1",
		"liferay-npm-bundler-preset-react": "1.8.0",
		"prettier-eslint-cli": "4.7.1",
		"properties": "1.2.1",
		"react-test-renderer": "16.6.3",
		"whatwg-fetch": "3.0.0"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/main/resources/META-INF/resources/js/**/components/**/*.js"
		],
		"coverageDirectory": "build/coverage",
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 80
			}
		},
		"moduleNameMapper": {
			"frontend-js-web/liferay/toast/commands/OpenToast.es": "<rootDir>/src/main/resources/META-INF/resources/js/test/mock/openToast.js"
		},
		"setupTestFrameworkScriptFile": "<rootDir>src/main/resources/META-INF/resources/js/test/setup.js",
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"testMatch": [
			"<rootDir>/src/main/resources/META-INF/resources/js/components/**/__tests__/*.js",
			"<rootDir>/src/main/resources/META-INF/resources/js/shared/components/**/__tests__/*.js"
		],
		"testURL": "http://liferay.com",
		"timers": "fake"
	},
	"main": "js/index.js",
	"name": "portal-workflow-metrics-web",
	"scripts": {
		"build": "babel --source-maps -d classes/META-INF/resources src/main/resources/META-INF/resources && liferay-npm-bundler",
		"csf": "csf src/main/resources/META-INF/resources/css/**/*.scss",
		"doc:code": "./node_modules/.bin/jsdoc -c ./jsdoc.json",
		"format": "prettier-eslint --write 'src/main/resources/META-INF/resources/js/**/*.js'",
		"lint": "eslint -c ./.eslintrc src/main/resources/META-INF/resources/js/ && npm run csf",
		"test": "cross-env TZ=Etc/GMT jest --coverage"
	},
	"version": "1.0.0"
}
