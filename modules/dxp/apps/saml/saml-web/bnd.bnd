Bundle-Name: Liferay SAML Web
Bundle-SymbolicName: com.liferay.saml.web
Bundle-Version: 2.0.0
Import-Package:\
	javax.net.ssl;resolution:=optional,\
	\
	*
Provide-Capability:\
	liferay.resource.bundle;\
		aggregate=saml-web;\
		bundle.symbolic.name=com.liferay.saml.web;\
		resource.bundle.aggregate:String="(&(bundle.symbolic.name=com.liferay.saml.web)(!(aggregate=*))),(bundle.symbolic.name=com.liferay.saml.api)";\
		resource.bundle.base.name="content.Language";\
		service.ranking="0"
Require-Capability:\
	liferay.resource.bundle;\
		filter:="(bundle.symbolic.name=com.liferay.saml.api)"
Web-ContextPath: /saml-web